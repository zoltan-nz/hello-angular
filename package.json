{
  "name": "hello-angular",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint --type-check --fix --format=stylish",
    "lint:check": "ng lint --type-check --format=stylish",
    "e2e:dev": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.dev.config.js jest --detectOpenHandles --runInBand --config ./e2e/jest.config.js",
    "e2e:ci": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.ci.config.js jest --runInBand --config ./e2e/jest.config.js",
    "format": "prettier --list-different --write '**/*.{ts,js,html,json,md}'",
    "format:check": "prettier --check '**/*.{ts,js,html,json,md}'",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ci:report:codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "ci:report:coveralls": "cat ./coverage/lcov.info | coveralls",
    "ci": "run-s -l build lint:check format:check test e2e:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "bootstrap": "4.5.0",
    "mobx": "^5.15.4",
    "mobx-angular": "^4.1.0",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@ngneat/spectator": "^5.12.0",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest": "^26.0.4",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/node": "^14.0.22",
    "@types/puppeteer": "^3.0.1",
    "chokidar-cli": "^2.1.0",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "expect-puppeteer": "^4.4.0",
    "husky": "^4.2.5",
    "jasmine-spec-reporter": "~5.0.2",
    "jest": "^26.1.0",
    "jest-environment-puppeteer": "^4.4.0",
    "jest-marbles": "^2.5.1",
    "jest-preset-angular": "8.2.1",
    "jest-puppeteer": "^4.4.0",
    "miragejs": "^0.1.40",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "puppeteer": "^5.0.0",
    "ts-jest": "^26.1.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p format:check lint:check test"
    }
  }
}
