{
  "name": "hello-angular",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint --type-check --fix --format=stylish",
    "lint:check": "ng lint --type-check --format=stylish",
    "e2e:dev": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.dev.config.js jest --detectOpenHandles --runInBand --config ./e2e/jest.config.js",
    "e2e:ci": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.ci.config.js jest --runInBand --config ./e2e/jest.config.js",
    "format": "prettier --list-different --write '**/*.{ts,js,html,json,md}'",
    "format:check": "prettier --check '**/*.{ts,js,html,json,md}'",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ci": "yarn && run-s -l build lint:check format:check test e2e:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "bootstrap": "^4.4.1",
    "mobx": "^5.15.4",
    "mobx-angular": "^4.1.0",
    "ngx-bootstrap": "^5.5.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@ngneat/spectator": "^5.3.1",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest": "^25.1.4",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^13.9.1",
    "@types/puppeteer": "^2.0.1",
    "chokidar-cli": "^2.1.0",
    "codelyzer": "^5.2.1",
    "expect-puppeteer": "^4.4.0",
    "husky": "^4.2.3",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^25.1.0",
    "jest-environment-puppeteer": "^4.4.0",
    "jest-marbles": "^2.5.1",
    "jest-preset-angular": "8.1.1",
    "jest-puppeteer": "^4.4.0",
    "miragejs": "zoltan-nz/miragejs",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.1",
    "ts-jest": "^25.2.1",
    "ts-node": "~8.6.2",
    "tslint": "6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p format:check lint:check test"
    }
  }
}
