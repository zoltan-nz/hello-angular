{
  "name": "hello-angular",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint --type-check --fix --format=stylish",
    "lint:check": "ng lint --type-check --format=stylish",
    "e2e:dev": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.dev.config.js jest --detectOpenHandles --runInBand --config ./e2e/jest.config.js",
    "e2e:ci": "JEST_PUPPETEER_CONFIG=./e2e/jest-puppeteer.ci.config.js jest --runInBand --config ./e2e/jest.config.js",
    "format": "prettier --list-different --write '**/*.{ts,js,html,json,md}'",
    "format:check": "prettier --check '**/*.{ts,js,html,json,md}'",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ci:report:codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "ci:report:coveralls": "cat ./coverage/lcov.info | coveralls",
    "ci": "run-s -l build lint:check format:check test e2e:ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.2",
    "@angular/common": "~9.1.2",
    "@angular/compiler": "~9.1.2",
    "@angular/core": "~9.1.2",
    "@angular/forms": "~9.1.2",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.0",
    "mobx": "^5.15.4",
    "mobx-angular": "^4.1.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.2",
    "@angular/language-service": "~9.1.2",
    "@ngneat/spectator": "^5.3.1",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^25.2.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^13.11.1",
    "@types/puppeteer": "^2.0.1",
    "chokidar-cli": "^2.1.0",
    "codacy-coverage": "^3.4.0",
    "codelyzer": "^5.2.2",
    "coveralls": "^3.0.11",
    "expect-puppeteer": "^4.4.0",
    "husky": "^4.2.5",
    "jasmine-spec-reporter": "^5.0.1",
    "jest": "^25.3.0",
    "jest-environment-puppeteer": "^4.4.0",
    "jest-marbles": "^2.5.1",
    "jest-preset-angular": "8.1.3",
    "jest-puppeteer": "^4.4.0",
    "miragejs": "^0.1.37",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "puppeteer": "^2.1.1",
    "ts-jest": "^25.3.1",
    "ts-node": "~8.8.2",
    "tslint": "6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p format:check lint:check test"
    }
  }
}
